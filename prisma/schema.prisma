generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Game {
  id          Int      @id @default(autoincrement())
  name        String
  genre       String
  rating      Int
  image       String?
  description String?
  createdAt   DateTime @default(now())
}
